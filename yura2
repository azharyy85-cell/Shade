    Callback = function(selectedPlayerName)
        TargetedPlayer = Players:FindFirstChild(selectedPlayerName)
        if TargetedPlayer then
            SendNotify("Target System", "تم تحديد الهدف: " .. TargetedPlayer.Name, 3)
        else
            SendNotify("Target System", "تم إلغاء تحديد الهدف", 3)
        end
    end
})

