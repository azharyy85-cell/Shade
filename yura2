local SelectedCar_InternalName = nil

local function spawnCar()
    if SelectedCar_InternalName then
        local args = {"PickingCar", SelectedCar_InternalName}
        game:GetService("ReplicatedStorage"):WaitForChild("RE"):WaitForChild("1Ca1r"):FireServer(unpack(args))
    end
end

