                    plrRoot.BreakVelocity:Destroy()
                end
            end)
        else
            StopAnim()
            local plrRoot = GetRoot(plr)
            if plrRoot and plrRoot:FindFirstChild("BreakVelocity") then
                plrRoot.BreakVelocity:Destroy()
            end
        end
    end
})

