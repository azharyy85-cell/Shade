    Name = "مسح جميع الادوات",
    Callback = function()
        local args = {
            [1] = "ClearAllTools"
        }
        game:GetService("ReplicatedStorage"):WaitForChild("RE"):WaitForChild("1Clea1rTool1s"):FireServer(unpack(args))
    end
})

-- Seção para equipar itens
Section = ToolsTab:AddSection({
    Name = "مسك"
})

-- Botão para equipar todos os itens
ToolsTab:AddButton({
    Name = "مسك جميع الادوات",
    Callback = function()
        for _, tool in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
            if tool:IsA("Tool") then
                tool.Parent = game.Players.LocalPlayer.Character
            end
        end
    end
})

-- Seção para Testes
Section = ToolsTab:AddSection({
    Name = "هاله"
})





                          
-- Botão para Fire N4zi
ToolsTab:AddButton({
    Name = "هاله دخان ",
    Description = "يسوي هاله دخان!",
    Callback = function()
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer

-- Caminho do FireX
local fireXPath = workspace:FindFirstChild("WorkspaceCom") 
    and workspace.WorkspaceCom:FindFirstChild("001_GiveTools") 
    and workspace.WorkspaceCom["001_GiveTools"]:FindFirstChild("FireX")

local nametools = "crystal nazi lel"
local oldcframe = LocalPlayer.Character.HumanoidRootPart.CFrame
local attempts = 57
local delayBetweenClicks = 0.3

-- Grip e Handle
local gripBase = CFrame.new(-0.290086746, 0.0755810738, -0.0109872818, 0.0439560413, 0.509705901, -0.859225094, -0.0591450632, -0.857220173, -0.511542261, -0.997281134, 0.0733042806, -0.00753343105)
local handleOffset = CFrame.new(2, 1, -3)

-- Velocidade absurda de rotação (sempre)
local rotSpeed = math.rad(3600) -- 3600°/s

-- Função de clique na ferramenta
local function clickTool(object)
    local clickDetector = object:FindFirstChildWhichIsA("ClickDetector")
    if clickDetector then
        fireclickdetector(clickDetector)
    end
end

-- Configura Grip e Handle
local function setupTool(tool)
    tool.Grip = gripBase
    tool.Name = nametools

    local handle = tool:FindFirstChild("Handle")
    if handle then
        local weld = handle:FindFirstChildWhichIsA("Motor6D")
        if weld then weld:Destroy() end

        -- HandleOffset fixo
        RunService.RenderStepped:Connect(function()
            if handle.Parent == LocalPlayer.Character then
                handle.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame * handleOffset
            end
        end)
    end

    tool.Parent = LocalPlayer.Character
end

-- Rotação contínua infinita
RunService.RenderStepped:Connect(function(deltaTime)
    local hrp = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if hrp then
        hrp.CFrame = hrp.CFrame * CFrame.Angles(0, rotSpeed * deltaTime, 0)
    end
end)

-- Função de duplicação do FireX
local function dupeToolsFireX(fireXPath)
    if not fireXPath then
        warn("FireX não encontrado.")
        return
    end

    local hrp = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if not hrp then return end

    hrp.CFrame = fireXPath.CFrame

    for i = 1, attempts do
        clickTool(fireXPath)
        task.wait(delayBetweenClicks)

        local tool = LocalPlayer.Backpack:FindFirstChild("FireX")
        if tool then
            setupTool(tool)
        end
    end

    hrp.CFrame = oldcframe
end

dupeToolsFireX(fireXPath)
    end
})

ToolsTab:AddButton({
    Name = "مطرقة تضهر للكل",
    Callback = function()

local tool = Instance.new("Tool")
tool.Name = "مطرقة تضهر لللكل"
tool.RequiresHandle = true
tool.CanBeDropped = true

local handle = Instance.new("Part")
handle.Name = "Handle"
handle.Size = Vector3.new(0.1, 0.1, 0.1)
handle.Massless = true
handle.Anchored = false
handle.CanCollide = false
handle.Transparency = 0.5
handle.Color = Color3.fromRGB(255, 0, 0)

local mesh = Instance.new("SpecialMesh", handle)
mesh.MeshType = Enum.MeshType.Sphere
mesh.Scale = Vector3.new(0.05, 0.05, 0.05)
mesh.Parent = handle

handle.Parent = tool

local hammerId = 15250088977 -- ايدي لمطرقة 

tool.Equipped:Connect(function(mouse)
 local character = tool.Parent
 if character and character:FindFirstChild("Humanoid") and character.Humanoid.RigType == Enum.HumanoidRigType.R15 then

  game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Wear"):InvokeServer(hammerId)

  mouse.Button1Down:Connect(function()
   local humanoid = character:FindFirstChildOfClass("Humanoid")
   local animator = humanoid and humanoid:FindFirstChildOfClass("Animator")
   if animator then
    local anim = Instance.new("Animation")
    anim.AnimationId = "rbxassetid://2410679501"-- هذا لانميشن ماضن تلاقي احسن منه
    local track = animator:LoadAnimation(anim)
    track:Play()
   end
  end)
 end
end)

tool.Unequipped:Connect(function()
 game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Wear"):InvokeServer(hammerId)
end)

tool.Parent = game.Players.LocalPlayer.Backpack


    end
})

ToolsTab:AddButton({
    Name = "مسجل اغاني يضهر للكل",
    Callback = function()


local player = game.Players.LocalPlayer
        local playerGui = player:FindFirstChild("PlayerGui")
        if not playerGui then return end

        local boombox
        local sg
        local lastID = 142376088

        local function createBoombox()
            boombox = Instance.new("Tool")
            boombox.Name = "مسجل مجاني"
            boombox.RequiresHandle = true
            boombox.Parent = player.Backpack

            local handle = Instance.new("Part")
            handle.Name = "Handle"
            handle.Size = Vector3.new(1, 1, 1)
            handle.CanCollide = false
            handle.Anchored = false
            handle.Transparency = 1
            handle.Parent = boombox

            boombox.Equipped:Connect(function()
                if sg then return end

                sg = Instance.new("ScreenGui")
                sg.Name = "ChooseSongGui"
                sg.Parent = playerGui  

                local frame = Instance.new("Frame")
                frame.Style = "RobloxRound"
                frame.Size = UDim2.new(0.25, 0, 0.25, 0)
                frame.Position = UDim2.new(0.375, 0, 0.375, 0)
                frame.Draggable = true
                frame.Active = true
                frame.Parent = sg

                local text = Instance.new("TextLabel")
                text.BackgroundTransparency = 1
                text.TextStrokeTransparency = 0
                text.TextColor3 = Color3.new(1, 1, 1)
                text.Size = UDim2.new(1, 0, 0.6, 0)
                text.TextScaled = true
                text.Text = "لاغاني تضهر للكل بعض لاغاني يصير بيها مشكل"
                text.Parent = frame

                local input = Instance.new("TextBox")
                input.BackgroundColor3 = Color3.new(0, 0, 0)
                input.BackgroundTransparency = 0.5
                input.BorderColor3 = Color3.new(1, 1, 1)
                input.TextColor3 = Color3.new(1, 1, 1)
                input.TextStrokeTransparency = 1
                input.TextScaled = true
                input.Text = tostring(lastID)
                input.Size = UDim2.new(1, 0, 0.2, 0)
                input.Position = UDim2.new(0, 0, 0.6, 0)
                input.Parent = frame

                local button = Instance.new("TextButton")
                button.Style = "RobloxButton"
                button.Size = UDim2.new(0.75, 0, 0.2, 0)
                button.Position = UDim2.new(0.125, 0, 0.8, 0)
                button.TextColor3 = Color3.new(1, 1, 1)
                button.TextStrokeTransparency = 0
                button.Text = "تشغيل!"
                button.TextScaled = true
                button.Parent = frame

                
                local args = { [1] = 18756289999 }
                game:GetService("ReplicatedStorage").Remotes.Wear:InvokeServer(unpack(args))

                local function playAudioAll(ID)
                    if type(ID) ~= "number" then
                        print("Please insert a valid number!")
                        return
                    end
                    local rs = game:GetService("ReplicatedStorage")
                    local evt = rs:FindFirstChild("1Gu1nSound1s", true)
                    if evt then
                        evt:FireServer(workspace, ID, 1)
                    end
                end

                local function playAudioLocal(ID)
                    local sound = Instance.new("Sound")
                    sound.SoundId = "rbxassetid://" .. ID
                    sound.Volume = 1
                    sound.Looped = false
                    sound.Parent = player.Character or workspace
                    sound:Play()
                    task.wait(3)
                    sound:Destroy()
                end

button.MouseButton1Click:Connect(function()
                    local soundID = tonumber(input.Text)
                    if soundID then
                        lastID = soundID
                        playAudioAll(soundID)
                        playAudioLocal(soundID)
                        if sg then
                            sg:Destroy()
                            sg = nil
                        end
                    else
                        print("Invalid ID!")
                    end
                end)
            end)

            boombox.Unequipped:Connect(function()
                if sg then
                    sg:Destroy()
                    sg = nil
                end
                local args = { [1] = 18756289999 }
                game:GetService("ReplicatedStorage").Remotes.Wear:InvokeServer(unpack(args))
            end)

            boombox.AncestryChanged:Connect(function(_, parent)
                if not parent and sg then
                    sg:Destroy()
                    sg = nil
                end
            end)
        end

        createBoombox()

    end
})

ToolsTab:AddToggle({
    Name = "سبام سيارات يملي السيرفر يضهر بس عندك",
    Default = false,
    Callback = function(state)
        toggleCar = state
        if toggleCar then
            task.spawn(function()
                while toggleCar do
                    local model = game:GetObjects("rbxassetid://8714153713")[1]
                    model.Parent = workspace
                    model:MoveTo(game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:GetPivot().Position + Vector3.new(0,5,0) or Vector3.zero)
                    task.wait(0.3)
                end
            end)
        end
    end
})

local BNumber = 600 
local raioBola = 10 
local passos = 12 

local function organizarItensEmBola()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local backpack = player.Backpack

    local itens = {}
    for _, item in pairs(character:GetChildren()) do
        if item:IsA("Tool") and item.Name == "FireX" then
            table.insert(itens, item)
        end
    end
    for _, item in pairs(backpack:GetChildren()) do
        if item:IsA("Tool") and item.Name == "FireX" then
            table.insert(itens, item)
        end
    end

    local usados = 0
    local totalItens = #itens

    for i = 0, passos do
        local theta = (i / passos) * math.pi -- de 0 a 180 graus
        local itensLinha = math.ceil(math.sin(theta) * passos * 2)
        if itensLinha == 0 then itensLinha = 1 end

        for j = 0, itensLinha - 1 do
            usados = usados + 1
            local item = itens[usados]
            if not item then break end

            item.Parent = character

            local phi = (j / itensLinha) * (2 * math.pi)
            local x = math.sin(theta) * math.cos(phi) * raioBola
            local y = math.cos(theta) * raioBola
            local z = math.sin(theta) * math.sin(phi) * raioBola

            item.GripPos = Vector3.new(x, y, z)
            task.wait()
            item.Parent = backpack
        end
    end

    for _, item in pairs(backpack:GetChildren()) do
        if item:IsA("Tool") and item.Name == "FireX" then
            item.Parent = character
        end
    end
end


ToolsTab:AddButton({
    Name = "صنع كرة طفايات حريق",
    Callback = function()
        local Player = game.Players.LocalPlayer
        local Character = Player.Character or Player.CharacterAdded:Wait()
        local RootPart = Character:WaitForChild("HumanoidRootPart")

 
        local Clone = game:GetService("Workspace"):FindFirstChild("WorkspaceCom")
            and game:GetService("Workspace").WorkspaceCom:FindFirstChild("001_GiveTools")
            and game:GetService("Workspace").WorkspaceCom["001_GiveTools"]:FindFirstChild("FireX")

        if Clone and Clone:FindFirstChild("ClickDetector") then
            local OldPos = RootPart.CFrame

            local processed = 0
            while processed < BNumber do
                RootPart.CFrame = Clone.CFrame
                fireclickdetector(Clone.ClickDetector)
                processed = processed + 1
                task.wait()
            end

            RootPart.CFrame = OldPos

            organizarItensEmBola()
        else
            warn("ملقيت الطفايات انسخ كمان")
        end
    end
})

ToolsTab:AddSection({ "ثقب اسود كبير جدا" })

local Players, RunService, Lighting, Workspace = game:GetService("Players"), game:GetService("RunService"), game:GetService("Lighting"), game:GetService("Workspace")
local Player = Players.LocalPlayer
local Character, Humanoid, RootPart
local blackHoleActive = false
local blackHoleModel = nil
local originalSky = nil

local function updateCharacter()
    Character = Player.Character or Player.CharacterAdded:Wait()
    if Character then
        Humanoid = Character:WaitForChild("Humanoid")
        RootPart = Character:WaitForChild("HumanoidRootPart")
    end
end

updateCharacter()
Player.CharacterAdded:Connect(updateCharacter)

function toggleBlackHole(ativar)
    blackHoleActive = ativar
    if blackHoleActive then
        originalSky = Lighting:FindFirstChildOfClass("Sky")
        if originalSky then originalSky.Parent = nil end
        local sky = Instance.new("Sky")
        sky.Name = "GargantuaSky"
        sky.SkyboxBk="rbxassetid://8139676647"
        sky.SkyboxDn="rbxassetid://8139676988"
        sky.SkyboxFt="rbxassetid://8139677111"
        sky.SkyboxLf="rbxassetid://8139677359"
        sky.SkyboxRt="rbxassetid://8139677253"
        sky.SkyboxUp="rbxassetid://8139676647"
        sky.Parent = Lighting
        blackHoleModel = Instance.new("Model", Workspace)
        blackHoleModel.Name = "GargantuaBlackHole"
        local singularity = Instance.new("Part", blackHoleModel)
        singularity.Shape = Enum.PartType.Ball
        singularity.Size = Vector3.new(80, 80, 80)
        singularity.Anchored = true
        singularity.CanCollide = false
        singularity.Material = Enum.Material.Neon
        singularity.Color = Color3.new(0, 0, 0)
        singularity.Transparency = 0.05
        local attachment = Instance.new("Attachment", singularity)
        local diskLayers = {}
        for i = 1, 7 do
            local disk = Instance.new("Part", blackHoleModel)
            disk.Shape = Enum.PartType.Cylinder
            disk.Size = Vector3.new(8, 200 + (i * 50), 200 + (i * 50))
            disk.Anchored = true
            disk.CanCollide = false
            disk.Material = Enum.Material.Neon
            disk.Color = Color3.fromHSV((i * 30) % 360 / 360 * 0.15, 1, 1)
            disk.Transparency = 0.3 + (i * 0.08)
            table.insert(diskLayers, disk)
            local light = Instance.new("PointLight", disk)
            light.Color = disk.Color
            light.Brightness = 8 - i
            light.Range = 300 + (i * 50)
        end
        local centralLight = Instance.new("PointLight", singularity)
        centralLight.Color = Color3.fromRGB(255, 100, 0)
        centralLight.Brightness = 15
        centralLight.Range = 800
        local function applyForce(v)
            if v:IsA("Part") and not v.Anchored and not v.Parent:FindFirstChild("Humanoid") and v.Parent ~= blackHoleModel then
                for _, force in pairs(v:GetChildren()) do
                    if force:IsA("BodyAngularVelocity") or force:IsA("BodyForce") or force:IsA("BodyVelocity") or force:IsA("AlignPosition") or force:IsA("Torque") then
                        force:Destroy()
                    end
                end
                v.CanCollide = false
                local torque = Instance.new("Torque", v)
                torque.Torque = Vector3.new(15000000, 15000000, 15000000)
                local align = Instance.new("AlignPosition", v)
                local att2 = Instance.new("Attachment", v)
                torque.Attachment0 = att2
                align.MaxForce = math.huge
                align.MaxVelocity = math.huge
                align.Responsiveness = 400
                align.Attachment0 = att2
                align.Attachment1 = attachment
            end
        end
        task.spawn(function()
            for i, v in pairs(Workspace:GetDescendants()) do
                if not blackHoleActive then break end
                applyForce(v)
                if i % 15 == 0 then task.wait() end
            end
        end)
        local descAddedConn = Workspace.DescendantAdded:Connect(function(v)
            if blackHoleActive then
                task.wait(0.1)
                applyForce(v)
            end
        end)
        local followConn = RunService.Heartbeat:Connect(function()
            if blackHoleActive and blackHoleModel.Parent and Player.Character and RootPart then
                local targetPos = RootPart.Position + Vector3.new(0, 50, 30)
                singularity.Position = singularity.Position:lerp(targetPos, 0.05)
                for _, disk in pairs(diskLayers) do
                    disk.Position = singularity.Position
                end
            else
                if followConn then followConn:Disconnect() end
            end
        end)
        local rotateConn = RunService.RenderStepped:Connect(function()
            if blackHoleActive and blackHoleModel.Parent then
                local rotationSpeed = tick() * math.rad(1.5)
                local pulseTime = tick() * 4
                for i, disk in pairs(diskLayers) do
                    if disk and disk.Parent then
                        local diskSpeed = rotationSpeed * (1 + i * 0.2)
                        disk.CFrame = CFrame.new(singularity.Position) * CFrame.Angles(0, 0, diskSpeed)
                    end
                end
                if centralLight then
                    local lightPulse = math.sin(pulseTime) * 0.3 + 1
                    centralLight.Brightness = 15 * lightPulse
                end
            else
                if rotateConn then rotateConn:Disconnect() end
            end
        end)
    else
        if blackHoleModel then
            blackHoleModel:Destroy()
            blackHoleModel = nil
        end
        local currentSky = Lighting:FindFirstChild("GargantuaSky")
        if currentSky then currentSky:Destroy() end
        if originalSky and not Lighting:FindFirstChildOfClass("Sky") then
            originalSky.Parent = Lighting
        end
    end
end

ToolsTab:AddToggle({
    Name = "تشغيل الثقب الاسود",
    Description = "يسوي ثقب اسود كبير فوقك ولونه اسود و الدنيا تصير لون برتقاليه و نار وكذا ",
    Default = false,
    Callback = function(Value)
        toggleBlackHole(Value)
    end
})

do
    local TargetTab = ToolsTab 
    
    TargetTab:AddSection({ "نسخ القنابل و تفجير" })

    local bomb_collectionTask = nil
    local bomb_detonationTask = nil
    local bomb_isCollecting = false
    local bomb_isDetonating = false

    local function stop_bomb_collection()
        if not bomb_isCollecting then return end
        bomb_isCollecting = false

        if bomb_collectionTask then
            task.cancel(bomb_collectionTask)
            bomb_collectionTask = nil
        end
        print("Bomb Collection: STOPPED")
    end

    local function start_bomb_collection()
        if bomb_isCollecting then return end
        bomb_isCollecting = true

        local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
        local RootPart = Character:WaitForChild("HumanoidRootPart")
        local Bomb = Workspace:WaitForChild("WorkspaceCom"):WaitForChild("001_CriminalWeapons"):WaitForChild("GiveTools"):WaitForChild("Bomb")

        if not Bomb or not RootPart then
            bomb_isCollecting = false
            return
        end
        
        bomb_collectionTask = task.spawn(function()
            RootPart.CFrame = Bomb.CFrame
            
            while bomb_isCollecting do
                pcall(function()
                    fireclickdetector(Bomb.ClickDetector)
                end)
                task.wait(0.01)
            end
        end)
        print("Bomb Collection: STARTED")
    end

    local function stop_bomb_detonation()
        if not bomb_isDetonating then return end
        bomb_isDetonating = false

        if bomb_detonationTask then
            task.cancel(bomb_detonationTask)
            bomb_detonationTask = nil
        end
        print("Bomb Detonation: STOPPED")
    end

    local function start_bomb_detonation()
        if bomb_isDetonating then return end
        bomb_isDetonating = true

        local DetonateRE = ReplicatedStorage:WaitForChild("RE"):WaitForChild("1Blo1wBomb1sServe1r")
        local detonateIdentifier = "Bomb" .. LocalPlayer.Name
        local detonateArgs = { [1] = detonateIdentifier }

        bomb_detonationTask = task.spawn(function()
            while bomb_isDetonating do
                pcall(function()
                    DetonateRE:FireServer(unpack(detonateArgs))
                end)
                task.wait(0.05)
            end
        end)
        print("Bomb Detonation: STARTED")
    end

    TargetTab:AddToggle({
        Name = "تشغيل/ايقاف نسخ.قنابل",
        Description = "لو تبي توقف نسخ اضغط مرا ثانيه عشان يوقف.",
        Default = false,
        Callback = function(state)
            if state then
                start_bomb_collection()
            else
                stop_bomb_collection()
            end
        end
    })

    TargetTab:AddToggle({
        Name = "تفجير القنابل",
        Description = "اضغط على الارض او باي مكان عشان يفجر قنابل لي معك",
        Default = false,
        Callback = function(state)
            if state then
                start_bomb_detonation()
            else
                stop_bomb_detonation()
            end
        end
    })
end

local AntiSecret = Window:MakeTab({
    Title = "الـمـضـادات",
    Icon = "plane"
})

AntiSecret:AddSection({ "- مـضـادات الـتـعـلـيـق و الـلاق -" })

AntiSecret:AddToggle({
    Name = "<font color='rgb(0, 255, 0)'>مضاد اللاق { مايصير لاق بالصملات }</font>",
    Default = false,
    Callback = function(value)
		if value then
			disableLaggyFeatures()
			connections["AntiLag"] = workspace.DescendantAdded:Connect(function(descendant)
				if descendant:IsA("ParticleEmitter") or descendant:IsA("Trail") or descendant:IsA("Smoke") or descendant:IsA("Fire") then
					descendant.Enabled = false
				end
			end)
		elseif connections["AntiLag"] then
			connections["AntiLag"]:Disconnect()
			connections["AntiLag"] = nil
		end
    end
})

local isIllusionV20Enabled = false

AntiSecret:AddToggle({
    Name = "مضاد بانج v2",
    Default = false,
    Callback = function(Value)
        isIllusionV20Enabled = Value
        if isIllusionV20Enabled then
            spawn(function()
                local player = game.Players.LocalPlayer
                local teleportSpeed = 1e-12
                local rotateSpeed = 999
                while isIllusionV20Enabled do
                    local HRP = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
                    if HRP then                        
                        HRP.CFrame = HRP.CFrame * CFrame.new(0, 1, 0)
                        
                        game:GetService("TweenService"):Create(HRP, TweenInfo.new(teleportSpeed), {
                            CFrame = HRP.CFrame * CFrame.new(0, -1, 0)
                        }):Play()
                        wait(teleportSpeed)                        
                        HRP.CFrame = HRP.CFrame * CFrame.Angles(math.rad(90), math.rad(180), math.rad(270))
                        game:GetService("TweenService"):Create(HRP, TweenInfo.new(1 / rotateSpeed), {
                            CFrame = HRP.CFrame * CFrame.Angles(math.rad(90), math.rad(180), math.rad(270))
                        }):Play()
                        wait(1 / rotateSpeed)
                    else
                        wait(0.1)
                    end
                end
            end)
        end
    end
})

AntiSecret:AddToggle({
    Name = "مضاد جلوس",
    Description = "يمنعك من الجلوس",
    Default = false,
    Callback = function(v)
        local plr = game.Players.LocalPlayer
        local hum = plr.Character and plr.Character:FindFirstChildOfClass("Humanoid")

        if v and hum then
            hum:SetStateEnabled(Enum.HumanoidStateType.Seated, false)
        elseif hum then
            hum:SetStateEnabled(Enum.HumanoidStateType.Seated, true)
        end

        plr.CharacterAdded:Connect(function(c)
            local h = c:WaitForChild("Humanoid")
            h:SetStateEnabled(Enum.HumanoidStateType.Seated, not v)
        end)
    end
})

AntiSecret:AddToggle({
    Name = "مضاد بانج V1",
    Default = false,
    Callback = function(isOn)
        local Players = game:GetService("Players")
        local lp = Players.LocalPlayer

        _G.__AntiBangHigh = _G.__AntiBangHigh or { enabled = false, thread = nil }

        local function getTopHeight()
            
            local maxY = 700 
            for _, obj in pairs(workspace:GetDescendants()) do
                if obj:IsA("BasePart") then
                    if obj.Position.Y > maxY then
                        maxY = obj.Position.Y
                    end
                end
            end
            return maxY + 50 
        end

        local function antiBangLoop()
            _G.__AntiBangHigh.enabled = true

            while _G.__AntiBangHigh.enabled do
                local char = lp.Character or lp.CharacterAdded:Wait()
                local hrp = char:FindFirstChild("HumanoidRootPart")

                if hrp then
                    local original = hrp.CFrame
                    local topY = getTopHeight()

                    hrp.CFrame = CFrame.new(original.X, topY, original.Z)
                    task.wait(0.2)

                    hrp.CFrame = original
                    task.wait(0.2)
                else
                    task.wait(0.3)
                end
            end
        end

        local function stopAntiBang()
            _G.__AntiBangHigh.enabled = false
        end

        if isOn then
            if not _G.__AntiBangHigh.enabled then
                _G.__AntiBangHigh.thread = task.spawn(antiBangLoop)
            end
        else
            stopAntiBang()
        end
    end
})

AntiSecret:AddToggle({
    Name = "مضاد افك (Afk)",
    Default = false,
    Callback = function(value)
        local player = game.Players.LocalPlayer
        if value then
        
            player.CharacterAdded:Connect(function(char)
                local hrp = char:WaitForChild("HumanoidRootPart")
                hrp.Anchored = false
            end)
            if player.Character then
                player.Character:WaitForChild("HumanoidRootPart").Anchored = false
            end
        else
            if player.Character then
                local hrp = player.Character:FindFirstChild("HumanoidRootPart")
                if hrp then
                    hrp.Anchored = false
                end
            end
        end
    end
})

AntiSecret:AddToggle({
    Name = "مضاد فلنق كرة",
    Default = false,
    Callback = function(value)
        local player = game.Players.LocalPlayer
        if value then
            
            game:GetService("RunService").Stepped:Connect(function()
                local char = player.Character
                if char and char:FindFirstChild("HumanoidRootPart") then
                    char.HumanoidRootPart.Velocity = Vector3.new(0, char.HumanoidRootPart.Velocity.Y, 0)
                end
            end)
        end
    end
    
    
})

local AntiFlingConnection = nil

AntiSecret:AddToggle({
    Name = "مضاد الفلنق",
    Description = "مضاد للفلنق",
    Default = false,
    Callback = function(Value)
        if Value then
            
            if AntiFlingConnection then return end 
            
            AntiFlingConnection = RunService.Stepped:Connect(function()
                for i, p in pairs(Players:GetPlayers()) do
                    task.spawn(function()
                        if p ~= LocalPlayer and p.Character then
                            for i, v in pairs(p.Character:GetChildren()) do
                                pcall(function()
                                    if v:IsA("BasePart") then
                                        v.CanCollide = false
                                        v.Velocity = Vector3.new(0, 0, 0)
                                        v.RotVelocity = Vector3.new(0, 0, 0)
                                        v.CustomPhysicalProperties = PhysicalProperties.new(0, 0, 0, 0, 0)
                                        v.Massless = true
                                    elseif v:IsA("Accessory") and v:FindFirstChild("Handle") then
                                        local handle = v.Handle
                                        handle.CanCollide = false
                                        handle.Velocity = Vector3.new(0, 0, 0)
                                        handle.RotVelocity = Vector3.new(0, 0, 0)
                                        handle.CustomPhysicalProperties = PhysicalProperties.new(0, 0, 0, 0, 0)
                                        handle.Massless = true
                                    end
                                end)
                            end
                        end
                    end)
                end
            end)
        else
            
            if AntiFlingConnection then
                AntiFlingConnection:Disconnect()
                AntiFlingConnection = nil
            end
        end
    end
})

local TargetTab = Window:MakeTab({
    Title = "التحكم باللاعبين (Target)",
    Icon = "rbxassetid://10734952036"
})

local Players = game:GetService("Players")
local plr = Players.LocalPlayer
local RunService = game:GetService("RunService")

local ScriptWhitelist = {}
local ForceWhitelist = {}
local TargetedPlayer = nil
local Velocity_Asset = Instance.new("BodyAngularVelocity")
Velocity_Asset.AngularVelocity = Vector3.new(0,0,0)
Velocity_Asset.MaxTorque = Vector3.new(50000,50000,50000)
Velocity_Asset.P = 1250
Velocity_Asset.Name = "BreakVelocity"

local function SendNotify(title, message, duration)
	game:GetService("StarterGui"):SetCore("SendNotification", {Title = title, Text = message, Duration = duration;})
end

local function GetCharacter(Player)
	if Player and Player.Character then
		return Player.Character
	end
end

local function GetRoot(Player)
    local char = GetCharacter(Player)
	if char and char:FindFirstChild("HumanoidRootPart") then
		return char.HumanoidRootPart
	end
end

local function GetPing()
	return (game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValue())/1000
end

local function TeleportTO(posX,posY,posZ,player,method)
	pcall(function()
        local plrRoot = GetRoot(plr)
        if not plrRoot then return end
		if method == "safe" then
			task.spawn(function()
				for i = 1,30 do
					task.wait()
					plrRoot.Velocity = Vector3.new(0,0,0)
					if player == "pos" then
						plrRoot.CFrame = CFrame.new(posX,posY,posZ)
					else
                        local targetRoot = GetRoot(player)
                        if targetRoot then
						    plrRoot.CFrame = CFrame.new(targetRoot.Position)+Vector3.new(0,2,0)
                        end
					end
				end
			end)
		else
			plrRoot.Velocity = Vector3.new(0,0,0)
			if player == "pos" then
				plrRoot.CFrame = CFrame.new(posX,posY,posZ)
			else
                local targetRoot = GetRoot(player)
                if targetRoot then
				    plrRoot.CFrame = CFrame.new(targetRoot.Position)+Vector3.new(0,2,0)
                end
			end
		end
	end)
end

local function PredictionTP(player,method)
    local plrRoot = GetRoot(plr)
	local root = GetRoot(player)
    if not plrRoot or not root then return end
	local pos = root.Position
	local vel = root.Velocity
	plrRoot.CFrame = CFrame.new((pos.X)+(vel.X)*(GetPing()*3.5),(pos.Y)+(vel.Y)*(GetPing()*2),(pos.Z)+(vel.Z)*(GetPing()*3.5))
	if method == "safe" then
		task.wait()
		plrRoot.CFrame = CFrame.new(pos)
		task.wait()
		plrRoot.CFrame = CFrame.new((pos.X)+(vel.X)*(GetPing()*3.5),(pos.Y)+(vel.Y)*(GetPing()*2),(pos.Z)+(vel.Z)*(GetPing()*3.5))
	end
end

local function GetPush()
	local TempPush = nil
	pcall(function()
		if plr.Backpack:FindFirstChild("Push") then
			local PushTool = plr.Backpack.Push
			PushTool.Parent = plr.Character
			TempPush = PushTool
		end
		for i,v in pairs(Players:GetPlayers()) do
			if v.Character and v.Character:FindFirstChild("Push") then
				TempPush = v.Character.Push
			end
		end
	end)
	return TempPush
end

local function Push(Target)
    if not Target or not Target.Character then return end
	local PushTool = GetPush()
	if PushTool and PushTool:FindFirstChild("PushTool") then
		local args = {[1] = Target.Character}
		PushTool.PushTool:FireServer(unpack(args))
	end
	if plr.Character and plr.Character:FindFirstChild("Push") then
		plr.Character.Push.Parent = plr.Backpack
	end
end

local function PlayAnim(id,time,speed)
	pcall(function()
        if not plr.Character then return end
		plr.Character.Animate.Disabled = false
		local hum = plr.Character:FindFirstChildOfClass("Humanoid")
        if not hum then return end
		local animtrack = hum:GetPlayingAnimationTracks()
		for i,track in pairs(animtrack) do
			track:Stop()
		end
		plr.Character.Animate.Disabled = true
		local Anim = Instance.new("Animation")
		Anim.AnimationId = "rbxassetid://"..id
		local loadanim = hum:LoadAnimation(Anim)
		loadanim:Play()
		loadanim.TimePosition = time
		loadanim:AdjustSpeed(speed)
		loadanim.Stopped:Connect(function()
            if plr.Character and plr.Character:FindFirstChild("Animate") then
			    plr.Character.Animate.Disabled = false
            end
		end)
	end)
end

local function StopAnim()
    if not plr.Character or not plr.Character:FindFirstChild("Animate") then return end
	plr.Character.Animate.Disabled = false
    local hum = plr.Character:FindFirstChildOfClass("Humanoid")
    if not hum then return end
    local animtrack = hum:GetPlayingAnimationTracks()
    for i, track in pairs (animtrack) do
        track:Stop()
    end
end

local function ToggleFling(bool)
	task.spawn(function()
		if bool then
			local RVelocity = nil
			while _G.isFlinging do
				pcall(function()
                    local plrRoot = GetRoot(plr)
                    if not plrRoot then return end
					RVelocity = plrRoot.Velocity 
					plrRoot.Velocity = Vector3.new(math.random(-1500,1500),-250000,math.random(-1500,1500))
					RunService.RenderStepped:wait()
					plrRoot.Velocity = RVelocity
				end)
				RunService.Heartbeat:wait()
			end
		end
	end)
end

TargetTab:AddSection({ Name = "اختيار اللاعب" })

local function getPlayerNames()
    local names = {}
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= plr then
            table.insert(names, player.Name)
        end
    end
    return names
end

